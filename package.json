{
  "name": "yomuki-demos",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "animejs": "^3.2.1",
    "dat.gui": "^0.7.9",
    "lil-gui": "^0.18.1",
    "vue": "^3.2.47",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.7",
    "@types/node": "^20.2.5",
    "@unocss/preset-uno": "^0.52.7",
    "@vitejs/plugin-vue": "^4.1.0",
    "@yomukizrj/eslint-config-json": "^0.1.0",
    "@yomukizrj/eslint-config-vue3-ts": "^1.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "typescript": "^5.0.2",
    "unocss": "^0.52.7",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        },
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ],
          "extensions": [
            ".ts",
            ".vue"
          ]
        }
      }
    },
    "extends": [
      "@yomukizrj/eslint-config-vue3-ts",
      "@yomukizrj/eslint-config-json",
      "./.eslintrc-auto-import.json"
    ],
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "\\/vite",
            "virtual:uno.css"
          ]
        }
      ]
    }
  }
}
